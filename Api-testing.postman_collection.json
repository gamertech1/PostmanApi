			"name": "Checking-status",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"var m1 = pm.response.json();\r",
							"console.log(m1[\"status\"]);\r",
							"\r",
							"pm.test(\"test 2\", ()=>{\r",
							"   pm.expect(m1.status).to.eql(\"OK\");\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
